{
    "name": "boco",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write .",
        "migration:new": "drizzle-kit generate:sqlite",
        "migration:run": "npx tsx src/lib/runMigrations.ts",
        "import:merci": "npx tsx src/lib/server/importing/merci/runImportMerci.ts"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@types/better-sqlite3": "^7.6.9",
        "@types/cli-progress": "^3.11.5",
        "@types/eslint": "8.56.0",
        "@types/nodemailer": "^6.4.14",
        "@types/underscore": "^1.11.15",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "drizzle-kit": "^0.20.13",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "prettier": "^3.2.4",
        "prettier-plugin-svelte": "^3.1.2",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "ts-node": "^10.9.2",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.3",
        "vitest": "^1.3.1"
    },
    "type": "module",
    "dependencies": {
        "@lucia-auth/adapter-drizzle": "^1.0.0",
        "@lucia-auth/adapter-sqlite": "^3.0.0",
        "better-sqlite3": "^9.3.0",
        "cli-progress": "^3.12.0",
        "drizzle-orm": "^0.29.3",
        "limiter-es6-compat": "^2.1.2",
        "lucia": "^3.0.1",
        "node-html-parser": "^6.1.12",
        "nodemailer": "^6.9.9",
        "oslo": "^1.0.4",
        "underscore": "^1.13.6"
    }
}
