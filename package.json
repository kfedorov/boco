{
	"name": "sveltekit-auth-start",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"compile": "paraglide-js compile",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.0.0-prerelease.14",
		"@inlang/paraglide-js-adapter-vite": "^1.0.1",
		"@skeletonlabs/skeleton": "^1.12.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.6",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/nodemailer": "^6.4.14",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"bun-types": "^1.0.15",
		"drizzle-kit": "^0.20.6",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.6.2",
		"sveltekit-superforms": "^0.8.7",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.5.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-ses": "^3.462.0",
		"@lucia-auth/adapter-sqlite": "^2.0.1",
		"@sentry/node": "^7.84.0",
		"@sentry/svelte": "^7.84.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"better-sqlite3": "^9.2.0",
		"drizzle-orm": "^0.29.1",
		"lucia": "^2.7.4",
		"lucide-svelte": "^0.294.0",
		"nodemailer": "^6.9.7"
	}
}
